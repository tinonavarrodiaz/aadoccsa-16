                    a.group1(href="img/syp/HABFUT.jpg")
                        img.imagenes-prod-item(src="img/syp/HABFUT.jpg")
                    a.link(target="_blank" href='assets/pdf/soler/F.T.FUTURE.pdf')
                      strong Ficha Técnica
